<%= render( 'layouts/errors', :object => @cure ) %>

<%= form_for [@ailment, @cure], remote: true do |f| %>
  <%= f.label :headline %>
  <%= f.text_field :headline %>
  <%= f.label :therapy %>
  <%= f.text_field :therapy %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :book_title %>
  <%= f.text_field :book_title %>
  <%= f.label :book_cover %>
  <%= f.text_field :book_cover %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <p><%= f.submit %></p>
<% end %>
